FROM quay.io/tsumahhh/suhail-whatsapp-bot
RUN git clone https://github.com/Tsumahhh/suhail-whatsapp-bot /root/smdd
RUN rm -rf /root/smdd/.git
WORKDIR /root/smdd
RUN npm install || yarn install
EXPOSE 8000
CMD ["npm","start" ] 
































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































